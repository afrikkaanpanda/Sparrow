<h1 class="title"><%= @user.first_name %>'s Profile</h1>

<p class="firstname">First Name: <%= @user.first_name %></p>
<p class="lastname">Last Name: <%= @user.last_name %></p>
<p class="username">Username: <%= @user.username %></p>

<%= button_to "Edit User", edit_user_path, method: :get %> 
<%= button_to "Home", root_path, method: :get %> 
<%= button_to "Logout", logout_path, method: :delete %> 